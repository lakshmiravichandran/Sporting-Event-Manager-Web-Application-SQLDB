<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
   version="2.5">
  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>com.demo.prj.HelloWorld</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/hello/*</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.demo.prj.LoginDetails</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login/*</url-pattern>
  </servlet-mapping>
      <servlet>
    <servlet-name>Signup</servlet-name>
    <servlet-class>com.demo.prj.Signup</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>Signup</servlet-name>
    <url-pattern>/signup/*</url-pattern>
  </servlet-mapping>
      <servlet>
    <servlet-name>EventDet</servlet-name>
    <servlet-class>com.demo.prj.EventDetails</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>EventDet</servlet-name>
    <url-pattern>/eventdetail/*</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>Analytics</servlet-name>
    <servlet-class>com.demo.prj.Analytics</servlet-class>
  </servlet>
    <servlet-mapping>
    <servlet-name>Analytics</servlet-name>
    <url-pattern>/analytics/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CityMap</servlet-name>
    <servlet-class>com.demo.prj.CityMap</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CityMap</servlet-name>
    <url-pattern>/cityMap/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>EventList</servlet-name>
    <servlet-class>com.demo.prj.EventList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventList</servlet-name>
    <url-pattern>/eventList/*</url-pattern>
  </servlet-mapping>

</web-app>
